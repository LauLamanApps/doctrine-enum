{
  "name": "laulamanapps/doctrine-enum",
  "description": "Create Doctrine Enum Types with ease",
  "license": "MIT",
  "authors": [
    {
      "name": "Laurens Laman",
      "email": "laurens.laman@llaman.nl"
    }
